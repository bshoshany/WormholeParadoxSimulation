    DownValues[$CellContext`radial1$$] = {HoldPattern[
         $CellContext`radial1$$[
          Pattern[$CellContext`t$, 
           Blank[]]]] :> Dynamic[
         If[$CellContext`persistentWormhole$$, 
          
          Sqrt[$CellContext`b$$^2 + ($CellContext`l$$ - $CellContext`v$$ \
$CellContext`t$)^2], 
          
          Sqrt[($CellContext`bump1$$[$CellContext`t$] + \
$CellContext`bump2$$[$CellContext`t$])^2 + ($CellContext`l$$ - \
$CellContext`v$$ $CellContext`t$)^2]]]}}, {
    DownValues[$CellContext`radial2$$] = {HoldPattern[
         $CellContext`radial2$$[
          Pattern[$CellContext`t$, 
           Blank[]]]] :> Dynamic[
         If[$CellContext`persistentWormhole$$, 
          
          Sqrt[$CellContext`b$$^2 + ($CellContext`v$$ ($CellContext`t$ + 
              2 - $CellContext`l$$/$CellContext`v$$))^2], 
          
          Sqrt[($CellContext`bump1$$[$CellContext`t$] + \
$CellContext`bump2$$[$CellContext`t$])^2 + ($CellContext`v$$ ($CellContext`t$ + 
              2 - $CellContext`l$$/$CellContext`v$$))^2]]]}}, {
    DownValues[$CellContext`geodesicPlot$$] = {HoldPattern[
