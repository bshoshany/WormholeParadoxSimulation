   ",", 
   RowBox[{
    RowBox[{
     RowBox[{"padNumber", "[", "n_", "]"}], ":=", 
     RowBox[{"PaddedForm", "[", 
      RowBox[{
       RowBox[{"N", "[", "n", "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"3", ",", "2"}], "}"}], ",", 
       RowBox[{"NumberPadding", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"\"\<\>\"", ",", "\"\<0\>\""}], "}"}]}], ",", 
       RowBox[{"NumberSigns", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"\"\<-\>\"", ",", "\"\<+\>\""}], "}"}]}]}], "]"}]}], ";", 
    RowBox[{
     RowBox[{"padDegrees", "[", "n_", "]"}], ":=", 
     RowBox[{"PaddedForm", "[", 
      RowBox[{
       RowBox[{"IntegerPart", "[", 
        RowBox[{"N", "[", "n", "]"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"3", ",", "0"}], "}"}], ",", 
       RowBox[{"NumberPadding", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"\"\< \>\"", ",", "\"\<0\>\""}], "}"}]}], ",", 
